<?xml version="1.0"?>
<block>
  <name>equisat_4fsk_block_decode</name>
  <key>equisat_decoder_equisat_4fsk_block_decode</key>
  <category>[equisat_decoder]</category>
  <import>import equisat_decoder</import>
  <make>equisat_decoder.equisat_4fsk_block_decode($max_num_blocks)</make>
  <!-- Make one 'param' node for every Parameter you want settable from the GUI.
       Sub-nodes:
       * name
       * key (makes the value accessible as $keyname, e.g. in the make node)
       * type -->
  <param>
    <name>Max # blocks</name>
    <key>max_num_blocks</key>
    <value>15</value>
    <type>int</type>
  </param>

  <!-- Make one 'sink' node per input. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <sink>
    <name>in</name>
    <type>message</type>
  </sink>

  <!-- Make one 'source' node per output. Sub-nodes:
       * name (an identifier for the GUI)
       * type
       * vlen
       * optional (set to 1 for optional inputs) -->
  <source>
    <name>out</name>
    <type>message</type>
  </source>
</block>
